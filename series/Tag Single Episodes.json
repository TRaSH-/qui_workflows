{
  "name": "Tag: Single Episodes",
  "trackerPattern": "*",
  "trackerDomains": [
    "*"
  ],
  "conditions": {
    "schemaVersion": "1",
    "tag": {
      "enabled": true,
      "tags": [
        "Episode"
      ],
      "mode": "add",
      "condition": {
        "operator": "OR",
        "conditions": [
          {
            "field": "NAME",
            "operator": "MATCHES",
            "value": "(?i)S\\d{1,3}E\\d{1,3}"
          },
          {
            "field": "NAME",
            "operator": "MATCHES",
            "value": "\\b\\d{4}\\D+\\d{2}\\D+\\d{2}\\b"
          }
        ]
      }
    }
  },
  "intervalSeconds": 60
}
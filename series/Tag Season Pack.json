{
  "name": "Tag: Season Pack",
  "trackerPattern": "*",
  "trackerDomains": [
    "*"
  ],
  "conditions": {
    "schemaVersion": "1",
    "tag": {
      "enabled": true,
      "tags": [
        "Season"
      ],
      "mode": "add",
      "condition": {
        "operator": "AND",
        "conditions": [
          {
            "field": "NAME",
            "operator": "MATCHES",
            "value": "(?i)(?:S\\d{1,3}|Season[\\s\\.]\\d{1,3})"
          },
          {
            "field": "NAME",
            "operator": "MATCHES",
            "value": "(?i)S\\d{1,3}E\\d{1,3}",
            "negate": true
          }
        ]
      }
    }
  }
}
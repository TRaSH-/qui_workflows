{
  "name": "Delete: problem cross-seeds",
  "trackerPattern": "*",
  "trackerDomains": [
    "*"
  ],
  "conditions": {
    "schemaVersion": "1",
    "delete": {
      "enabled": true,
      "mode": "deleteWithFilesPreserveCrossSeeds",
      "condition": {
        "operator": "AND",
        "conditions": [
          {
            "field": "CATEGORY",
            "operator": "EQUAL",
            "value": "cross-seed"
          },
          {
            "field": "ADDED_ON_AGE",
            "operator": "GREATER_THAN_OR_EQUAL",
            "value": "3600"
          },
          {
            "field": "STATE",
            "operator": "NOT_EQUAL",
            "value": "tracker_down"
          },
          {
            "operator": "OR",
            "conditions": [
              {
                "field": "STATE",
                "operator": "EQUAL",
                "value": "errored"
              },
              {
                "field": "STATE",
                "operator": "EQUAL",
                "value": "missingFiles"
              },
              {
                "operator": "OR",
                "conditions": [
                  {
                    "field": "STATE",
                    "operator": "EQUAL",
                    "value": "tracker_down"
                  },
                  {
                    "field": "STATE",
                    "operator": "EQUAL",
                    "value": "stopped"
                  },
                  {
                    "operator": "AND",
                    "conditions": [
                      {
                        "field": "PROGRESS",
                        "operator": "GREATER_THAN_OR_EQUAL",
                        "value": "0.96"
                      },
                      {
                        "field": "PROGRESS",
                        "operator": "LESS_THAN",
                        "value": "1"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  },
  "intervalSeconds": 3600
}